<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.joonzis.mapper.RestMapper">
	<select id="findAll" resultType="org.joonzis.domain.RestVO">
		SELECT * FROM TBL_REST
	</select>
	
	<select id="findByNo" resultType="org.joonzis.domain.RestVO">
		SELECT * FROM TBL_REST WHERE REST_NO = #{rest_no}
	</select>
</mapper>