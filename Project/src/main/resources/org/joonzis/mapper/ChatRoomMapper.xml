<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="org.joonzis.mapper.ChatRoomMapper"> <!-- 경로 표기법이 클래스명 표기법처럼 사용된다! -->
	
	<!-- 채팅방 생성 쿼리 -->
	<insert id="createChatRoom" parameterType="org.joonzis.domain.ChatRoomVO">
	    INSERT INTO GROUP_CHAT (CHAT_NO, LINE_NO, MEM_NO, CHAT_LOG)
	    VALUES (1, #{groupNo}, #{memNo}, #{chatTitle})
</insert>
</mapper>






